    "recharts": "^2.10.3",
    "swagger-ui-react": "^5.18.2",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.17",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "xmldom": "0.6.0",
    "zod": "^3.22.4"
