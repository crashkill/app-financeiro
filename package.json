    "eslint-plugin-react-refresh": "^0.4.5",
    "fake-indexeddb": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sass": "^1.83.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.7.2",
    "vite": "^5.0.8"
  }
