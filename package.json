{
  "name": "app-financeiro",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "set NODE_ENV=production && tsc && vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "test": "vitest",
    "test:watch": "jest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "clean": "rimraf dist",
    "deploy:vercel": "npm run build:prod && vercel --prod",
    "doppler:setup": "doppler setup",
    "doppler:dev": "doppler run -- npm run dev",
    "doppler:start": "doppler run -- npm start",
    "doppler:build": "doppler run -- npm run build",
    "doppler:status": "node doppler-recovery.js status",
    "doppler:recover": "node doppler-recovery.js recover",
    "doppler:test": "node doppler-recovery.js test",
    "doppler:secrets": "doppler secrets",
    "mcp:test": "doppler run -- node -e \"console.log('Testando MCP com Doppler:', JSON.stringify(process.env, null, 2))\"",
    "mcp:sync": "node scripts/sync-mcp-config.js",
    "mcp:sync:all": "node scripts/sync-mcp-config.js all",
    "mcp:sync:cursor": "node scripts/sync-mcp-config.js cursor",
    "mcp:sync:generate": "node scripts/sync-mcp-config.js generate"
  },
  "dependencies": {
    "@apollo/client": "^4.0.3",
    "@handsontable/react": "^15.0.0",
    "@hookform/resolvers": "^3.3.4",
    "@iconscout/react-unicons": "^2.2.1",
    "@mui/icons-material": "^6.3.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.17.9",
    "@tanstack/react-virtual": "^3.11.2",
    "@types/react-select": "^5.0.0",
    "@types/swagger-ui-react": "^4.18.3",
    "@types/xmldom": "0.1.34",
    "autoprefixer": "^10.4.20",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.7",
    "chartjs-plugin-annotation": "^3.1.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "dexie": "^4.0.10",
    "dexie-react-hooks": "^1.1.7",
    "firebase": "^10.7.1",
    "framer-motion": "^12.15.0",
    "graphql": "^16.11.0",
    "hamburger-react": "^2.5.2",
    "handsontable": "^15.0.0",
    "lru-cache": "^11.0.2",
    "lucide-react": "^0.303.0",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.7",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.49.2",
